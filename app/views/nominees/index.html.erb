<p id="notice"><%= notice %></p>

<h1>Nominees</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Category</th>
      <th>Description</th>
      <th>Status</th>
      <th>Avatar2</th>
      <th>User</th>
      <th>Avatar2 url</th>
      <th>Image3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nominees.each do |nominee| %>
      <tr>
        <td><%= nominee.name %></td>
        <td><%= nominee.category_id %></td>
        <td><%= nominee.description %></td>
        <td><%= nominee.status %></td>
        <td><%= nominee.avatar2 %></td>
        <td><%= nominee.user_id %></td>
        <td><%= nominee.avatar2_url %></td>
        <td><%= nominee.image3 %></td>
        <td><%= link_to 'Show', nominee %></td>
        <td><%= link_to 'Edit', edit_nominee_path(nominee) %></td>
        <td><%= link_to 'Destroy', nominee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nominee', new_nominee_path %>
